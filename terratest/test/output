# Source: pega/templates/pega-batch-hpa.yaml

# The Horizontal Pod Autoscaler for pega-batch deployment
apiVersion: autoscaling/v2beta1
kind: HorizontalPodAutoscaler
metadata:
  name: "pega-batch-hpa"
  namespace: default
spec:
  scaleTargetRef:
    apiVersion: extensions/v1beta1
    kind: Deployment
    name: "pega-batch"
  minReplicas: 1
  maxReplicas: 3
  metrics:
  - type: Resource
    resource:
      name: cpu
      targetAverageUtilization: 700
  - type: Resource
    resource:
      name: memory
      targetAverageUtilization: 85

